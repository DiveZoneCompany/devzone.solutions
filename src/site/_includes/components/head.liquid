<head>
  <!-- STYLING -->
  <link rel="stylesheet" href="/css/{{ manifest['style.css'] }}">
  <!-- FAV ICON -->
  <link rel="apple-touch-icon" sizes="180x180" href="/favicon/apple-touch-icon.png">
  <link rel="icon" type="image/png" sizes="32x32" href="/favicon/favicon-32x32.png">
  <link rel="icon" type="image/png" sizes="16x16" href="/favicon/favicon-16x16.png">
  <link rel="manifest" href="/manifest.json">
  <!-- FONT AWESOME -->
  <link rel="stylesheet" href="/fontawesome/css/fontawesome.min.css">
  <link rel="stylesheet" href="/fontawesome/css/thin.min.css">
  <link rel="stylesheet" href="/fontawesome/css/brands.min.css">
  <!-- PAGE DATA -->
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <title>{{ title | default: manifest.title }}</title>
  <meta name="description" content="{{ description | default: manifest.meta.description }}">
  <meta name="keywords" content="{{ manifest.meta.keywords | concat: tags | join: ', ' }}">
  <meta name="author" content="{{ author | default: manifest.meta.author }}">
  <!-- FEEDS -->
  <link rel="alternate" href="/feed/feed.xml" type="application/atom+xml" title="{{ manifest.title }}">
  <link rel="alternate" href="/feed/feed.json" type="application/json" title="{{ manifest.title }}">
  <!-- ANALYTICS -->
  {% include './analytics.liquid' %}
</head>
